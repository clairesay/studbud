   width: auto;\r\n            height: calc(100% - 60px);\r\n            margin-top: 14px;\r\n\r\n            overflow-y: auto;\r\n            scroll-snap-type: y mandatory;\r\n\r\n            // SCROLLBAR\r\n            &::-webkit-scrollbar {\r\n                width: 24px; /* width of the entire scrollbar */\r\n            }\r\n        \r\n            &::-webkit-scrollbar-track {\r\n                background: transparent; /* color of the tracking area */\r\n            }\r\n        \r\n            &::-webkit-scrollbar-thumb {\r\n                background-color: $grey; /* color of the scroll thumb */\r\n                border-radius: $big-radius; /* roundness of the scroll thumb */\r\n                border: 8px solid $light-grey; /* creates padding around scroll thumb */\r\n            }\r\n            \r\n            &::-webkit-scrollbar-thumb:hover {\r\n                background-color: $dark-grey;\r\n            }\r\n\r\n            // actual task cards\r\n            article.card {\r\n                z-index: 10;\r\n                border-radius: $big-radius;\r\n                background-color: $white;\r\n                padding: 16px;\r\n                filter: drop-shadow(0 0 2px #30303018);\r\n                margin: 16px 0;\r\n                scroll-behavior: smooth;\r\n                scroll-snap-align: start;\r\n\r\n                *:hover {\r\n                    cursor: initial;\r\n                }\r\n    \r\n                &:hover {\r\n                    cursor: grab;\r\n                }\r\n\r\n                &:first-of-type {\r\n                    margin: 2px 0 0 0;\r\n                }\r\n\r\n                &:last-child {\r\n                    margin-bottom: 72px;\r\n                }\r\n\r\n                // edit task button\r\n                .edit {\r\n                    padding: 6px;\r\n                    position: absolute;\r\n                    top: 12px;\r\n                    right: 16px;\r\n                    opacity: 0.4;\r\n                    \r\n                    \r\n                    &:hover {\r\n                        cursor: pointer;\r\n                        background-color: $dark-white;\r\n                        border-radius: $mini-radius;\r\n                        opacity: 1;\r\n                        \r\n                    }\r\n                    svg:hover, path:hover {\r\n                        cursor: pointer;\r\n                    }\r\n                }\r\n\r\n                h3 {\r\n                    line-height: 20px;\r\n                    margin-top: 4px;\r\n                    width: 85%;\r\n                    white-space: nowrap;\r\n                    overflow: hidden;\r\n                    text-overflow: ellipsis;\r\n                    font-size: 18px;\r\n                    font-weight: 500;\r\n                }\r\n\r\n                span.subject {\r\n                    font-size: 12px;\r\n                    padding: 3px 17px;\r\n                    margin: 12px 0;\r\n                    border-radius: $big-radius;\r\n                    text-transform: uppercase;\r\n                    border: 1.2px solid $black;\r\n                    & ~ h3 {\r\n                        margin: 16px 0 8px 0;\r\n                    }\r\n                }\r\n\r\n                div.time-details {\r\n                    display: flex;\r\n                }\r\n\r\n                p {\r\n                    margin: 8px 0 16px 0;\r\n                    font-size: 14px;\r\n                }\r\n                hr {\r\n                    border: none;\r\n                    border-top: 2px solid $light-grey;\r\n                }\r\n                div.time-details {\r\n                    padding-top: 6px;\r\n                    display: flex;\r\n                    align-items: center;\r\n                    \r\n\r\n                    h4 {\r\n                        padding-left: 12px;\r\n                        font-size: 14px;\r\n                        font-weight: 400;\r\n                        color: $dark-grey;\r\n                        text-transform: uppercase;\r\n                    }\r\n                    span.time {\r\n                        font-weight: 500;\r\n                        font-size: 12px;\r\n                        margin-left: auto;\r\n                        color: $dark-grey;\r\n                        padding: 4px 18px;\r\n                        border-radius: $big-radius;\r\n                        background-color: $light-grey;\r\n                    }\r\n                }\r\n\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n#tasks.list {\r\n    // background-color: blue;\r\n    display: flex;\r\n    flex-direction: column;\r\n    scroll-snap-type: y mandatory;\r\n    overflow: scroll scroll;\r\n    div.column {\r\n        background-color: transparent;\r\n        height: auto;\r\n        width: auto;\r\n        max-width: 1000px;\r\n        margin: 36px 36px 0 36px;\r\n        padding: 0;\r\n        box-shadow: none;\r\n        div.title {\r\n            padding: 0 0 12px 0;\r\n            * {\r\n                display: none;\r\n            }\r\n            input, h3.total {\r\n                display: inline;\r\n            }\r\n        }\r\n\r\n        // div.column.column:first-of-type {\r\n        //     margin-left: 0;\r\n        // }\r\n\r\n        div.cards {\r\n            height: auto;\r\n            width: auto;\r\n            margin-top: 0;\r\n            padding: 2px;\r\n            \r\n            article.card {\r\n                background-color: $white;\r\n                display: flex;\r\n                margin: 0 0 36px 0;\r\n                flex-direction: row;\r\n                justify-content: space-between;\r\n                align-items: center;\r\n                padding: 6px;\r\n                // filter: none;\r\n                span.tag.subject ~ h3, h3 {\r\n                    max-width: max-content;\r\n                    margin: auto 12px;\r\n                }\r\n                hr {\r\n                    display: none;\r\n                }\r\n                p {\r\n                    margin: auto;\r\n                    margin-left: 0;\r\n                }\r\n\r\n                p {\r\n                    text-overflow: ellipsis;\r\n                    white-space: nowrap;\r\n                    overflow: hidden;\r\n                  \r\n                    border-radius: $mini-radius;\r\n                }\r\n                span.tag.subject {\r\n                    margin: auto 12px;\r\n                    padding: 3px 17px;\r\n                }\r\n\r\n                div.time-details {\r\n                    margin-left: auto;\r\n                    padding: 0;\r\n                    justify-content: flex-end;\r\n                    min-width: 250px;\r\n                    span.tag.time {\r\n                        margin: auto 12px;\r\n                    }\r\n                }\r\n\r\n                .edit {\r\n                    padding: 6px;\r\n                    // position: absolute;\r\n                    // top: 12px;\r\n                    // right: 16px;\r\n                    top: 0;\r\n                    right: 0;\r\n                    opacity: 0.4;\r\n                    position: relative;\r\n                    // left: 0;\r\n                }\r\n\r\n                &:last-child, & {\r\n                    margin-bottom: 12px;\r\n                }\r\n            }\r\n        }\r\n\r\n    }\r\n}\r\n\r\n// #tasks.list div.column.column:first-of-type, #tasks.list div.column.column:last-of-type {\r\n//     margin-left: 0;\r\n//     margin-right: 0;\r\n// }\r\n\r\n// SCROLLABLE CARD MODIFICATIONS\r\n.chosen-card {\r\n    background-color: $white;\r\n}\r\n\r\n.sortable-fallback {\r\n    opacity: 1 !important;\r\n}\r\n\r\n.sortable-drag {\r\n    cursor: grabbing;\r\n}\r\n\r\n.ghost-card, .ghost-column {\r\n    opacity: 0.5;\r\n    border: 2px dashed $dark-grey;\r\n\r\n    * {\r\n        background-color: transparent;\r\n        color: transparent;\r\n        fill: transparent;\r\n        opacity: 0;\r\n    }\r\n}\r\n","// CONTENT CARDS\r\n// flexbox source https://developer.mozilla.org/en-US/docs/Web/CSS/CSS_Flexible_Box_Layout/Mastering_Wrapping_of_Flex_Items\r\n#content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    overflow: scroll auto;\r\n\r\n    // SCROLLBAR\r\n    &::-webkit-scrollbar {\r\n        width: 18px; /* width of the entire scrollbar */\r\n    }\r\n\r\n    &::-webkit-scrollbar-track {\r\n        background: $light-grey; /* color of the tracking area */\r\n    }\r\n\r\n    &::-webkit-scrollbar-thumb {\r\n        background-color: $grey; /* color of the scroll thumb */\r\n        border-radius: $big-radius; /* roundness of the scroll thumb */\r\n        border: 4px solid $light-grey; /* creates padding around scroll thumb */\r\n    }\r\n    \r\n    &::-webkit-scrollbar-thumb:hover {\r\n        background-color: $dark-grey;\r\n    }\r\n\r\n    div.group {\r\n        display: flex;\r\n        flex-direction: column;\r\n        margin: 36px;\r\n\r\n        h3.open-link {\r\n            font-weight: 400;\r\n            font-size: 14px;\r\n            text-transform: uppercase;\r\n            color: $dark-grey;\r\n            align-self: center;\r\n            display: flex;\r\n            align-items: center;\r\n            padding: 8px;\r\n            border-radius: $mini-radius;\r\n\r\n            svg {\r\n                margin-left: 6px;\r\n            }\r\n            &:not(.link-absent):hover {\r\n                cursor: pointer;\r\n                background-color: $light-grey;\r\n  \r\n            }\r\n        }\r\n\r\n        div.group-title {\r\n            display: flex;\r\n            justify-content: space-between;\r\n\r\n            background-color: $dark-white;\r\n            padding: 12px 0;\r\n            border-radius: $big-radius;\r\n\r\n            h3, input {\r\n                font-weight: 500;\r\n                font-size: 16px;\r\n                color: $dark-grey;\r\n            }\r\n\r\n            input {\r\n                width: auto;\r\n                background-color: transparent;\r\n                border: none;\r\n                padding: 2px;\r\n                margin-right: 12px;\r\n\r\n                &:hover {\r\n                    cursor: text;\r\n\r\n                    & ~ svg.edit-group {\r\n                        background-color: $light-grey;\r\n                    }\r\n                }\r\n            }\r\n\r\n            div {\r\n                display: flex;\r\n                position: relative;\r\n\r\n                svg.first {\r\n                    display: none\r\n                }\r\n\r\n                svg {\r\n                    padding: 8px;\r\n                    border-radius: $mini-radius;\r\n                    &.disabled path {\r\n                        fill: $grey\r\n\r\n                    }\r\n                    &:not(.disabled):hover {\r\n                        cursor: pointer;\r\n                        background-color: $light-grey;\r\n                    }\r\n                    &.edit-group {\r\n                        margin-right: 6px;\r\n\r\n                        &:hover ~ div.tooltip#edit-group {\r\n                            opacity: 1\r\n                        }\r\n                    }\r\n\r\n                    &.delete-group {\r\n                        &:not(.disabled):hover ~ div.tooltip#delete-group {\r\n                            opacity: 1\r\n                        }\r\n                    }\r\n                }\r\n\r\n                div.tooltip {\r\n                    font-size: 12px;\r\n                    justify-content: center;\r\n                    top: -32px;\r\n                    opacity: 0;\r\n                    width: 104px;\r\n                    background-color: $black;\r\n                    color: $white;\r\n                    text-align: center;\r\n                    border-radius: $mini-radius;\r\n                    padding: 6px 0;\r\n                    position: absolute;\r\n                    z-index: 1;\r\n                    transition-property: opacity;\r\n                    transition-duration: 0.2s;\r\n    \r\n                    &#edit-group {\r\n                        right: 0px;\r\n                    }\r\n                    &#delete-group {\r\n                        right: -37px;\r\n                    }\r\n    \r\n                    &::after {\r\n                        content: \"\";\r\n                        position: absolute;\r\n                        top: 100%;\r\n                        left: 50%;\r\n                        margin-left: -5px;\r\n                        border-width: 5px;\r\n                        border-style: solid;\r\n                        border-color: $black transparent transparent transparent;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        div.tiles {\r\n            display: flex;\r\n            flex-wrap: wrap;\r\n            margin: -12px;\r\n            min-height: 90px;\r\n\r\n            article.tile {\r\n                min-width: 260px;\r\n                max-width: 280px;\r\n                flex: 1 1 280px;\r\n                margin: 12px;\r\n                padding: 16px;\r\n                height: max-content;\r\n\r\n                border-radius: $big-radius;\r\n                background-color: $white;\r\n                \r\n                filter: drop-shadow(0 0 2px #30303018);\r\n\r\n                a.edit-content {\r\n                    padding: 6px;\r\n                    position: absolute;\r\n                    top: 12px;\r\n                    right: 16px;\r\n                    opacity: 0.4;\r\n                    \r\n                    \r\n                    &:hover {\r\n                        cursor: pointer;\r\n                        background-color: $dark-white;\r\n                        border-radius: $mini-radius;\r\n                        opacity: 1;\r\n                        \r\n                    }\r\n                }\r\n\r\n                svg {\r\n                    display: flex;\r\n                }\r\n\r\n                h3 {\r\n                    width: 85%;\r\n                    white-space: nowrap;\r\n                    overflow: hidden;\r\n                    text-overflow: ellipsis;                  \r\n                    font-size: 18px;\r\n                    font-weight: 500;\r\n                    margin: 4px 0 16px 0;\r\n                }\r\n\r\n                // code snippets\r\n                pre {\r\n                    font-size: 14px;\r\n                    line-height: 20px;\r\n                    margin-bottom: 12px;\r\n                    code {\r\n                        white-space: pre-wrap;\r\n                        text-overflow: ellipsis;\r\n                        border-radius: $mini-radius;\r\n                        overflow-wrap: break-word;\r\n                        word-wrap: break-word;\r\n                        word-break: break-all;\r\n                        padding: 12px;\r\n                    }\r\n                }\r\n\r\n                hr {\r\n                    border: none;\r\n                    border-top: 2px solid $light-grey;\r\n                }\r\n\r\n                a.external-link {\r\n                    color: $dark-grey;\r\n                    margin: 12px 0 4px 0;\r\n                    font-size: 14px;\r\n                    display: flex;\r\n                    flex-direction: row-reverse;\r\n                    justify-content: flex-end;\r\n                    align-items: center;\r\n                    overflow-wrap: break-word;\r\n                    word-wrap: break-word;\r\n                    word-break: break-all;\r\n\r\n                    svg {\r\n                        padding-right: 6px;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n","// BUTTON FORMATTING\r\n.button {\r\n    min-width: 96px;\r\n    padding: 12px 18px;\r\n    border-radius: $mini-radius;\r\n    font-weight: 500;\r\n    font-size: 14px;\r\n  \r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n\r\n    // variations\r\n    &.active {\r\n        visibility: visible;\r\n    }\r\n\r\n    &:hover {\r\n        cursor: pointer;\r\n    }\r\n\r\n    // adding a bit of space between the buttons\r\n    &:not(:last-child) {\r\n        margin-right: 16px;\r\n    }\r\n\r\n    // BUTTON TYPES\r\n    &.primary {\r\n        color: $white;\r\n        background-color: $primary;\r\n\r\n        &:hover {\r\n            background-color:$primary-hover\r\n        }\r\n\r\n        // hiding the expanded buttons for mobile\r\n        &.icon {\r\n            display: none;\r\n            min-width: auto;\r\n        }\r\n    }\r\n\r\n    // secondary grey button\r\n    &.secondary {\r\n        background-color: $light-grey;\r\n\r\n        &:hover {\r\n            background-color:#dddddd;\r\n        }\r\n    }\r\n\r\n    // red delete button\r\n    &.danger {\r\n        background-color: $danger;\r\n        color: $white;\r\n        &:hover {\r\n            background-color: $danger-hover;\r\n        }\r\n    }\r\n\r\n}\r\n","// FORMS\r\n\r\n// modal background\r\n.background {\r\n    display: none;\r\n    background-color: #0e1318af;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    z-index: 5;\r\n}\r\n\r\n// CODE\r\ncode {\r\n    font-family:'Courier New', Courier, monospace;\r\n}\r\n\r\n// FORM\r\nform {\r\n    z-index: 8;\r\n    display: none;\r\n    position: absolute;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    width: 80%;\r\n    left: 50%;\r\n    transform: translate(-50%, 0);\r\n    padding: 36px;\r\n    flex-direction: column;\r\n    background-color: $white;\r\n    border-radius: $big-radius;\r\n\r\n    max-width: 720px;\r\n\r\n    &.active {\r\n        display: flex;\r\n    }\r\n\r\n    &#add-column-form,\r\n    &#add-group-form {\r\n        top: 50%;\r\n        left: 50%;\r\n        transform: translate(-50%, -50%);\r\n        max-width: 360px;\r\n        input {\r\n            width:auto;\r\n        }\r\n        p {\r\n            font-size: 12px;\r\n            color: $dark-grey;\r\n            margin: 24px 0 12px 0;\r\n        }\r\n        h1.validate-message {\r\n            margin: 18px 0;\r\n        }\r\n        .form-button-container {\r\n            padding-top: 18px;\r\n            margin-right: 0;\r\n            button.primary {\r\n                margin-right: 0;\r\n            }\r\n        }\r\n    }\r\n\r\n    section {\r\n        display: grid;\r\n        grid-template-columns: 4fr 3fr 2px;\r\n        column-gap: 24px;\r\n\r\n        div:first-child {\r\n            input {\r\n                width: 80%;\r\n            }\r\n            textarea {\r\n                width: 80%;\r\n                border: none;\r\n                padding: 12px;\r\n                resize: vertical;\r\n                filter: drop-shadow(0 0 1px rgba(48, 48, 48, 0.18));\r\n                border-radius: $mini-radius;\r\n                &:hover {\r\n                    filter: drop-shadow(0 0 2px #30303048);\r\n                }\r\n            }\r\n        }\r\n    }\r\n    pre {\r\n        display: flex;\r\n        width: 80%;\r\n    }\r\n    // unformatted code in form\r\n    textarea[name=\"conceptDescription\"] {\r\n        font-family:'Courier New', Courier, monospace;\r\n        max-height: 30%;\r\n        height: 100px;\r\n        color: $white;\r\n        min-width: 100%;\r\n        max-width: 100%;\r\n        font-size: 14px;\r\n        padding: 12px;\r\n        background-color: $black;\r\n        filter: drop-shadow(0 0 1px rgba(48, 48, 48, 0.18));\r\n        white-space: pre-wrap;\r\n        text-overflow: ellipsis;\r\n        border-radius: $mini-radius;\r\n        overflow-wrap: break-word;\r\n        word-wrap: break-word;\r\n        word-break: break-all;\r\n        &:hover {\r\n            filter: drop-shadow(0 0 2px #30303048);\r\n        }\r\n    }\r\n    code:empty:before {\r\n        content:attr(data-text);\r\n        color: $grey;\r\n    }\r\n\r\n    select {\r\n        appearance: none;\r\n        padding: 12px;\r\n        width: 70%;\r\n        border-radius: $mini-radius;\r\n        border: none;\r\n        filter: drop-shadow(0 0 1px rgba(48, 48, 48, 0.18));\r\n        background-color: $white;\r\n        &:hover {\r\n            filter: drop-shadow(0 0 2px #30303048);\r\n            cursor: pointer;\r\n        }\r\n    }\r\n\r\n    .form-button-container {\r\n        display: flex;\r\n        flex-direction: row-reverse;\r\n        padding-top: 36px;\r\n        margin-right: 10px;\r\n        button {\r\n            align-self: flex-end;\r\n        }\r\n    }\r\n\r\n    h1 {\r\n        font-size: 24px;\r\n        font-weight: 500;\r\n        &.validate-message {\r\n            font-size: 14px;\r\n            margin-top: 36px;\r\n            font-weight: 400;\r\n            color: $danger;\r\n            text-align: right;\r\n            padding-right: 26px;\r\n        }\r\n    }\r\n\r\n    button {\r\n        &#edit-task-delete,\r\n        &#edit-content-delete {\r\n            display: none;\r\n            margin-right: auto;\r\n        }\r\n\r\n        &.close-form {\r\n            position: absolute;\r\n            top: 24px;\r\n            right: 24px;\r\n            padding: 12px;\r\n            display: flex;\r\n            justify-content: center;\r\n\r\n            background-color: $white;\r\n            border-radius: $mini-radius;\r\n\r\n            &:hover {\r\n                background-color: $light-grey;\r\n                cursor: pointer;\r\n            }\r\n        }\r\n    }\r\n\r\n    &.update {\r\n        button {\r\n            &#edit-task-delete,\r\n            &#edit-content-delete {\r\n                display: flex;\r\n            }\r\n        }\r\n    }\r\n\r\n    input {\r\n        border: none;\r\n        filter: drop-shadow(0 0 1px rgba(48, 48, 48, 0.18));\r\n        border-radius: $mini-radius;\r\n        padding: 12px;\r\n        font-size: 14px;\r\n        background-color: $white;\r\n        width: 60%;\r\n\r\n        &:required, &:required:hover, &:required:focus {\r\n            filter: drop-shadow(0 0 2px $danger);\r\n        }\r\n\r\n        &:hover {\r\n            filter: drop-shadow(0 0 2px #30303048);\r\n        }\r\n        &:focus {\r\n            filter: drop-shadow(0 0 2px #30303048);\r\n        }\r\n\r\n        &[name=\"taskEstimatedTimeHr\"],\r\n        &[name=\"taskEstimatedTimeMin\"] {\r\n            font-size: 14px;\r\n            width: 70px;\r\n        }\r\n\r\n        &[type=\"radio\"] {\r\n            display: none;\r\n\r\n            ~ span {\r\n                font-size: 14px;\r\n                padding: 12px 18px;\r\n                display: flex;\r\n                border-radius: $mini-radius;\r\n                background-color: $white;\r\n                filter: drop-shadow(0 0 1px rgba(48, 48, 48, 0.18));\r\n\r\n                &:hover {\r\n                    filter: drop-shadow(0 0 2px #30303048);\r\n                    cursor: pointer;\r\n                }\r\n\r\n                div {\r\n                    width: 12px;\r\n                    height: 12px;\r\n                    border-radius: 12px;\r\n\r\n                    margin-right: 8px;\r\n                }\r\n            }\r\n            &:checked ~ span {\r\n                background-color: $black;\r\n                color: $white;\r\n            }\r\n        }\r\n    }\r\n\r\n    .radio-container {\r\n        display: flex;\r\n        gap: 12px;\r\n    }\r\n\r\n    label {\r\n        display: block;\r\n        font-size: 12px;\r\n        text-transform: uppercase;\r\n        padding-left: 12px;\r\n        margin: 24px 0 4px 0;\r\n        color: $dark-grey;\r\n\r\n        &[for=\"taskEstimatedTimeMin\"] {\r\n            position: relative;\r\n            left: -10px;\r\n        }\r\n\r\n        &[for=\"taskEstimatedTimeHr\"],\r\n        &[for=\"taskEstimatedTimeMin\"] {\r\n            margin: 0;\r\n            padding: 0;\r\n            display: initial;\r\n            width: 70px;\r\n            span {\r\n                position: relative;\r\n                right: 60px;\r\n            }\r\n        }\r\n\r\n        &[for=\"taskPriorityRating\"] {\r\n            display: flex;\r\n            flex-direction: column;\r\n        }\r\n\r\n        &.false-radio {\r\n            margin: 0;\r\n            padding: 0;\r\n\r\n            &:hover {\r\n                cursor: pointer;\r\n            }\r\n\r\n            span {\r\n                background-color: $white;\r\n                color: $dark-grey;\r\n                padding: 14px;\r\n            }\r\n        }\r\n\r\n        span.required {\r\n            color: $danger;\r\n        }\r\n    }\r\n}\r\n","// RESPONSIVE CSS\r\n\r\n@media only screen and (max-width: 900px) {\r\n    #tasks.list div.column div.cards article.card p {\r\n        display: none;\r\n    }\r\n}\r\n\r\n@media only screen and (max-width: 700px) {\r\n    // LOGO/HOME-LINK\r\n    #home-link {\r\n        display: none;\r\n    }\r\n\r\n    header {\r\n        height: 48px;\r\n    }\r\n\r\n    // STRUCTURE\r\n    body main .tab#tasks-tab {\r\n        left: 16px;\r\n        content: \"Tasks\";\r\n    }\r\n\r\n    body main .tab#content-tab {\r\n        left: 128px;\r\n        content: \"Content\";\r\n    }\r\n\r\n    #tasks {\r\n        div.column {\r\n            min-width: 260px;\r\n            width: 75vw;\r\n            max-width: 332px;\r\n        }\r\n    }\r\n\r\n    #tasks.list div.column {\r\n        div.title {\r\n            padding: 0 0 6px 0;\r\n        }\r\n        div.cards article.card {\r\n            flex-direction: column;\r\n            align-items: flex-start;\r\n            padding: 16px;\r\n            filter: drop-shadow(0 0 2px #30303018);\r\n            margin: 16px 0;\r\n            span.tag.subject {\r\n                margin: 0;\r\n            }\r\n            div.time-details span.tag.time {\r\n                margin-right: 0;\r\n            }\r\n            span.tag.subject ~ h3 {\r\n                max-width: initial;\r\n                margin: 12px 0 0 0;\r\n                white-space: normal;\r\n            }\r\n            hr {\r\n                display: inline;\r\n            }\r\n            p {\r\n                // text-overflow: ellipsis;\r\n                white-space: normal;\r\n                overflow: auto;\r\n                display: inline;\r\n                max-width: 100%;\r\n                margin: 6px 0 12px 0;\r\n            }\r\n            .edit {\r\n                position: absolute;\r\n                top: 12px;\r\n                right: 16px;\r\n            }\r\n        }\r\n    }\r\n\r\n    // FORM\r\n    form {\r\n        &#add-column-form,\r\n        &#add-group-form {\r\n            max-width: 80%;\r\n        }\r\n        section {\r\n            grid-template-columns: auto;\r\n            grid-template-rows: auto auto;\r\n        }\r\n    }\r\n\r\n    /* /////////// COMPONENTS //////////// */\r\n    // BUTTON\r\n    .button-container {\r\n        flex-direction: column;\r\n        align-items: flex-end;\r\n        position: absolute;\r\n        top: 12px;\r\n        right: 12px;\r\n        z-index: 5;\r\n\r\n        .button {\r\n            &.primary {\r\n                &.icon {\r\n                    display: flex;\r\n                    width: 48px;\r\n                    height: 48px;\r\n                    &:hover {\r\n                        background-color: $primary-hover;\r\n                    }\r\n                }\r\n            }\r\n\r\n            &:not(:last-child) {\r\n                margin-right: 0;\r\n                margin-bottom: 12px;\r\n            }\r\n\r\n            &.create {\r\n                background-color: $white;\r\n                color: $black;\r\n                display: none;\r\n                visibility: hidden;\r\n\r\n                &:hover {\r\n                    background-color: $light-grey;\r\n                }\r\n            }\r\n\r\n            &.active {\r\n                display: flex;\r\n                visibility: visible;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media only screen and (max-width: 500px) {\r\n    form {\r\n        section {\r\n            div:first-child,\r\n            div {\r\n                input,\r\n                textarea,\r\n                pre {\r\n                    width: calc(100% - 24px);\r\n                }\r\n            }\r\n        }\r\n        div.form-button-container {\r\n            margin: 0;\r\n            button:first-child {\r\n                margin: 0;\r\n            }\r\n        }\r\n    }\r\n    // list view for tasks\r\n\r\n    #tasks.list div.column,\r\n    #tasks.list div.column:first-child,\r\n    #tasks.list div.column:last-child {\r\n        margin: 12px;\r\n    }\r\n\r\n    #content {\r\n        div.group {\r\n            div.group-title {\r\n                justify-content: space-between;\r\n                // flex-direction: column;\r\n                // div {\r\n                //     justify-content: space-between;\r\n                //     svg.edit-group {\r\n                //         margin-left: auto;\r\n                //     }\r\n                // }\r\n                h3.open-link {\r\n                    font-size: 0;\r\n                }\r\n            }\r\n\r\n        }\r\n\r\n    }\r\n\r\n    // popups\r\n    .pop-up[state=\"peek\"] {\r\n        width: 180px;\r\n\r\n        &#time {\r\n            #dropdown {\r\n                display: none;\r\n            }\r\n        }\r\n        &#music {\r\n            nav #current-controls {\r\n                .metadata {\r\n                    width: 70%;\r\n                }\r\n                .controls {\r\n                    #next,\r\n                    #previous {\r\n                        display: none;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    .pop-up[state=\"standby\"] {\r\n        h1#peek-time {\r\n            display: none;\r\n        }\r\n    }\r\n\r\n    .pop-up[state=\"active\"]#time,\r\n    .pop-up[state=\"active\"]#music {\r\n        position: fixed;\r\n        z-index: 4;\r\n        right: 0;\r\n        width: calc(100vw - 32px);\r\n        padding: auto;\r\n\r\n        #stopwatch {\r\n            h1 {\r\n                margin-left: calc((100% - 225px) / 2);\r\n                left: 0;\r\n            }\r\n            h1 ~ div {\r\n                margin: auto;\r\n            }\r\n        }\r\n    }\r\n}\r\n",".pop-up {\r\n    position: absolute;\r\n    bottom: 17px;\r\n    width: 280px;\r\n    background-color: $white;\r\n    border-radius: $big-radius $big-radius 0 0;\r\n    padding: 16px;\r\n    filter: drop-shadow(0 0 2px #30303018);\r\n    z-index: 3;\r\n\r\n    nav {\r\n        display: flex;\r\n        align-items: center;\r\n        margin-bottom: 36px;\r\n        h1 {\r\n            display: none;\r\n            position: absolute;\r\n            left: 60px;\r\n            padding: 12px;\r\n            font-weight: 500;\r\n            font-size: 18px;\r\n            &#peek-status {\r\n                display: none;\r\n                left: auto;\r\n                right: 24px;\r\n                text-transform: capitalize;\r\n                font-size: 16px;\r\n                color: $dark-grey;\r\n            }\r\n            \r\n        }\r\n        img {\r\n            margin-left: 12px;\r\n        }\r\n    }\r\n\r\n    button.icon {\r\n        padding: 8px;\r\n        min-width: auto;\r\n        margin-left: auto;\r\n        background-color: $white;\r\n        &:hover {\r\n            background-color: $light-grey;\r\n        }\r\n    }\r\n}\r\n.pop-up[state=active] {\r\n    &#music {\r\n        nav {\r\n            img {\r\n                display: none;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.pop-up[state=standby] {\r\n    height: 36px;\r\n    width: 36px;\r\n    display: flex;\r\n\r\n    &#time {\r\n        right: 110px;\r\n        nav, nav img {\r\n            display: flex;\r\n            margin-left: 0;\r\n            margin: auto;\r\n        }\r\n        *, nav *:not(img), div ~ h1#peek-time, #stowatch.active, #stopwatch, #pomodoro.active, #pomodoro {\r\n            display: none;\r\n        }\r\n        nav h1 {\r\n            display: none !important\r\n        }\r\n    }\r\n\r\n    &#music {\r\n        nav, nav img {\r\n            display: flex;\r\n            margin-left: 0;\r\n            margin: auto;\r\n        }\r\n        *, nav *:not(img), #current-controls {\r\n            display: none\r\n        }\r\n    }\r\n}\r\n\r\n.pop-up[state=peek] {\r\n    height: 36px;\r\n    &#time {\r\n        nav {\r\n            .collapse {\r\n                visibility: hidden;\r\n            }\r\n            #peek-status {\r\n                display: flex;\r\n            }\r\n        }\r\n        #stopwatch, #pomodoro {\r\n            display: none;\r\n        }\r\n    }\r\n\r\n    &#music {\r\n        nav {\r\n            display: flex;\r\n            #current-controls {\r\n                width: 100%;\r\n                .metadata {\r\n                    width: 50%;\r\n                    h1.title {\r\n                        width: 100%;\r\n                        display: flex;\r\n                        position: relative;\r\n                        padding: 0;\r\n                        left: 0;\r\n                    }\r\n                }\r\n                .controls {\r\n                    position: absolute;\r\n                    right: 18px;\r\n                }\r\n            }\r\n    \r\n            img{\r\n                display: none;\r\n            }\r\n            .collapse {\r\n                display: none;\r\n            }\r\n        }\r\n    \r\n        #current, h2, .playlist {\r\n            display: none;\r\n        }\r\n    }\r\n}\r\n\r\n@keyframes load-spans {\r\n    from {width: 0}\r\n    to {width: inherit}\r\n}","#empty-state-tasks, #empty-state-content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 16px;\r\n    border-radius: $big-radius;\r\n    border: 2px dashed $dark-grey;\r\n    img {\r\n        width: 60%;\r\n        margin: 24px auto;\r\n    }\r\n    h3 {\r\n        font-size: 18px;\r\n        font-weight: 500;\r\n    }\r\n    p {\r\n        color: $dark-grey;\r\n        margin: 8px 0 16px 0;\r\n        font-size: 14px;\r\n        line-height: 17px;\r\n    }\r\n}\r\n\r\n#empty-state-content {\r\n    min-width: 260px;\r\n    max-width: 280px;\r\n    flex: 1 1 280px;\r\n    margin: 12px;\r\n}","@import \"reset\";\r\n@import \"global\";\r\n@import \"components/music\";\r\n@import \"components/time\";\r\n@import \"components/structure\"; \r\n@import \"components/kanban\"; \r\n@import \"components/content\"; \r\n@import \"components/buttons\"; \r\n@import \"components/form\"; \r\n@import \"components/media-queries\"; \r\n@import \"components/pop-up\";\r\n@import \"components/empty-states\"\r\n"],"names":[],"version":3,"file":"index.HASH_REF_80038805bd6829e42a01d0f20c72b7db.css.map"}