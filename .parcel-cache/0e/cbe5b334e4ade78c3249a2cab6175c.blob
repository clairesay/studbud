ative;\r\n\r\n            &:hover svg {\r\n                visibility: visible;\r\n            }\r\n\r\n            h3,input {\r\n                font-weight: 500;\r\n                font-size: 16px;\r\n                color: $dark-grey;\r\n            }\r\n\r\n            // total\r\n            h3 {\r\n                position: absolute;\r\n                right: 12px;\r\n            }\r\n\r\n            // column title\r\n            input {\r\n                width: calc(100% - 80px);\r\n                background-color: transparent;\r\n                border: none;\r\n                padding: 2px;\r\n                text-decoration: underline dotted transparent;\r\n\r\n                &.column-name {    \r\n                    &:hover ~ svg.edit-column {\r\n                        background-color: $light-grey;\r\n                    }\r\n\r\n                    &:focus ~ svg,\r\n                    &:active ~ svg,\r\n                    &:hover ~ svg {\r\n                        visibility: visible;\r\n                    }\r\n                }\r\n            }\r\n            \r\n            div.tooltip {\r\n                font-size: 12px;\r\n                justify-content: center;\r\n                opacity: 0;\r\n                width: 104px;\r\n                background-color: $black;\r\n                color: $white;\r\n                text-align: center;\r\n                border-radius: $mini-radius;\r\n                padding: 6px 0;\r\n                position: absolute;\r\n                z-index: 1;\r\n                transition-property: opacity;\r\n                transition-duration: 0.2s;\r\n\r\n                &#edit {\r\n                    right: 14px;\r\n                    top: -28px; \r\n                }\r\n\r\n                &#delete {\r\n                    right: -22px;\r\n                    top: -28px;\r\n                }\r\n\r\n                &::after {\r\n                    content: \"\";\r\n                    position: abso