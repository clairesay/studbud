a {\r\n                width: 50%;\r\n                h1.title {\r\n                    width: 100%;\r\n                    display: flex;\r\n                    position: relative;\r\n                    // max-width: 55%;\r\n                    padding: 0;\r\n                    left: 0;\r\n                }\r\n            }\r\n            .controls {\r\n                position: absolute;\r\n                right: 18px;\r\n            }\r\n        }\r\n\r\n        img{\r\n            display: none;\r\n        }\r\n        .collapse {\r\n            display: none;\r\n        }\r\n    }\r\n\r\n    #current, h2, .playlist {\r\n        display: none;\r\n    }\r\n}\r\n\r\n#final-load {\r\n    display: none;\r\n    width: 248px;\r\n    height: 20px;\r\n    background-color: $grey;\r\n    div {\r\n        background-color: $primary;\r\n    }\r\n}\r\n\r\n@keyframes load-spans {\r\n    from {width: 0}\r\n    to {width: inherit}\r\n}\r\n\r\n@media only screen and (max-width: 700px) {\r\n\r\n}\r\n\r\n@media only screen and (max-width: 450px) {\r\n    .pop-up[state=\"peek\"] {\r\n        width: 180px;\r\n\r\n        &#time {\r\n            #dropdown {\r\n                display: none;\r\n            }\r\n        }\r\n        &#music {\r\n\r\n            nav #current-controls {\r\n                .metadata {\r\n                    width: 70%;\r\n                }\r\n                .controls {\r\n                    #next, #previous {\r\n                        display: none;\r\n                    }\r\n                }\r\n            }\r\n\r\n        }\r\n    }\r\n\r\n    .pop-up[state=\"standby\"] {\r\n        h1#peek-time {\r\n            display: none;\r\n        }\r\n    }\r\n\r\n    .pop-up[state=\"active\"]#time, .pop-up[state=\"active\"]#music {\r\n        position: fixed;\r\n        z-index: 4;\r\n        right: 0;\r\n        width: calc(100vw - 32px);\r\n        padding: auto;\r\n\r\n        #stopwatch {\r\n            h1 {\r\n                margin: auto;\r\n     