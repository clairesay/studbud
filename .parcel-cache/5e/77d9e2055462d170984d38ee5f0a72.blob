ty: visible;\r\n        }\r\n    }\r\n\r\n    div.cards {\r\n      width: auto;\r\n        height: calc(100% - 60px);\r\n        margin-top: 14px;\r\n\r\n        overflow-y: auto;\r\n        scroll-snap-type: y mandatory;\r\n        article.card {\r\n            z-index: 10;\r\n            border-radius: $big-radius;\r\n            background-color: $white;\r\n            padding: 16px;\r\n            filter: drop-shadow(0 0 2px #30303018);\r\n            margin: 16px 0;\r\n            scroll-behavior: smooth;\r\n            scroll-snap-align: start;\r\n        }\r\n        article.card:last-child {\r\n          margin-bottom: 24px;\r\n        }\r\n    \r\n        article.card:first-of-type {\r\n            margin: 2px 0 0 0;\r\n        }\r\n\r\n    }\r\n}\r\n\r\ndiv.column:first-of-type {\r\n    margin-left: 36px;\r\n}\r\n\r\ndiv.column:last-of-type {\r\n  margin-right: 36px;\r\n}\r\n\r\n        /* ///////////// SCROLL BAR //////////// */\r\n        div.cards::-webkit-scrollbar {\r\n            width: 24px;            /* width of the entire scrollbar */\r\n        }\r\n    \r\n        div.cards::-webkit-scrollbar-track {\r\n            background: transparent;        /* color of the tracking area */\r\n        }\r\n    \r\n        div.cards::-webkit-scrollbar-thumb {\r\n            background-color: $grey;    /* color of the scroll thumb */\r\n            border-radius: $big-radius;       /* roundness of the scroll thumb */\r\n            border: 8px solid $light-grey;  /* creates padding around scroll thumb */\r\n        }\r\n\r\n        #tasks::-webkit-scrollbar {\r\n          width: 8px;               /* width of the entire scrollbar */\r\n      }\r\n  \r\n      #tasks::-webkit-scrollbar-track {\r\n          background: $light-grey;        /* color of the tracking area */\r\n      }\r\n  \r\n      #tasks::-webkit-scrollbar-thumb {\r\n          background-color: $grey;    /* color of the scroll thumb */\r\n          border-radius: $big-radius;       /* roundness 