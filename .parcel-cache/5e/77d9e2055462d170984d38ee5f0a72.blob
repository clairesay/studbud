derline dotted transparent;\r\n        }\r\n  \r\n        input:focus {\r\n          text-decoration: none;\r\n        }\r\n  \r\n        input.column-name:hover {\r\n          cursor: text;\r\n          text-decoration: underline dotted $dark-grey;\r\n        }\r\n    }\r\n      div.tiles {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        margin:-12px;\r\n        article.tile {\r\n          min-width: 260px;\r\n          max-width: 280px;\r\n          flex: 1 1 280px;\r\n          margin: 12px;\r\n          border-radius: $big-radius;\r\n          background-color: $white;\r\n          padding: 16px;\r\n          filter: drop-shadow(0 0 2px #30303018);\r\n        }\r\n      }\r\n    }\r\n  }\r\n  \r\n  ","// BUTTON FORMATTING\r\n.button {\r\n    padding: 12px 18px;\r\n    border-radius: $mini-radius;\r\n    font-weight: 500;\r\n    font-size: 14px;\r\n  \r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n\r\n    // variations\r\n    &.active {\r\n        visibility: visible;\r\n    }\r\n\r\n    &:hover {\r\n        cursor: pointer;\r\n    }\r\n\r\n    // adding a bit of space between the buttons\r\n    &:not(:last-child) {\r\n        margin-right: 16px;\r\n    }\r\n\r\n    // BUTTON TYPES\r\n    &.primary {\r\n        color: $white;\r\n        background-color: $primary;\r\n\r\n        &:hover {\r\n            background-color:$primary-hover\r\n        }\r\n\r\n        // hiding the expanded buttons for mobile\r\n        &.icon {\r\n            display: none;\r\n        }\r\n    }\r\n\r\n    &.secondary {\r\n        background-color: $light-grey;\r\n\r\n        &.hover {\r\n            background-color:#dddddd\r\n        }\r\n    }\r\n\r\n}\r\n",".edit:hover {\r\n    background-color: greenyellow;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .edit {\r\n    padding: 18px;\r\n  }\r\n  \r\n  form#create-task-form {\r\n    button#create-task-submit {\r\n      display: flex;\r\n    }\r\n    button#edit-task-save, butt