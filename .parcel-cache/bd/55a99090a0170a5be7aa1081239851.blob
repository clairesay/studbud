         &:hover ~ svg {\r\n                        visibility: visible;\r\n                    }\r\n                }\r\n            }\r\n\r\n            svg {\r\n                position: relative;\r\n                \r\n                background-color: $dark-white;\r\n                visibility: hidden;\r\n\r\n                &.edit-column {\r\n                    margin-right: 6px;\r\n                }\r\n                &.edit-column,\r\n                &.delete-column {\r\n                    background-color: $dark-white;\r\n                    right: -12px;\r\n                    padding: 8px;\r\n                    border-radius: $mini-radius;\r\n\r\n                    &:hover {\r\n                        background-color: $light-grey;\r\n                    }\r\n                }\r\n            }\r\n\r\n        }\r\n        \r\n        div.cards {\r\n            width: auto;\r\n            height: calc(100% - 60px);\r\n            margin-top: 14px;\r\n\r\n            overflow-y: auto;\r\n            scroll-snap-type: y mandatory;\r\n\r\n            // SCROLLBAR\r\n            &::-webkit-scrollbar {\r\n                width: 24px; /* width of the entire scrollbar */\r\n            }\r\n        \r\n            &::-webkit-scrollbar-track {\r\n                background: transparent; /* color of the tracking area */\r\n            }\r\n        \r\n            &::-webkit-scrollbar-thumb {\r\n                background-color: $grey; /* color of the scroll thumb */\r\n                border-radius: $big-radius; /* roundness of the scroll thumb */\r\n                border: 8px solid $light-grey; /* creates padding around scroll thumb */\r\n            }\r\n            \r\n            &::-webkit-scrollbar-thumb:hover {\r\n                background-color: $dark-grey;\r\n            }\r\n\r\n            article.card {\r\n                z-index: 10;\r\n                border-radius: $big-radius;\r\n                background-color: $white;\r\n                padding: 16px;\r\n                filter: drop-shadow(0 0 2px #30303018);\r\n                margin: 16px 0;\r\n                scroll-behavior: smooth;\r\n                scroll-snap-align: start;\r\n\r\n                &:first-of-type {\r\n                    margin: 2px 0 0 0;\r\n                }\r\n\r\n                &:last-child {\r\n                    margin-bottom: 24px;\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n        div#empty-state-message {\r\n            flex-direction: column;\r\n            position: absolute;\r\n            top: 20%;\r\n            // max-width: calc(100% - 18px);\r\n\r\n            h4, img, button {\r\n                max-width: calc(100% - 18px);\r\n            }\r\n\r\n            h4 {\r\n                text-align: center;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n// div.time-icon {\r\n//     width: 12px;\r\n//     height: 12px;\r\n//     border-radius: 10px;\r\n// }\r\n\r\n// SCROLLABLE CARD MODIFICATIONS\r\n.chosen-card {\r\n    background-color: $white;\r\n}\r\n\r\n.sortable-fallback {\r\n    opacity: 1 !important;\r\n}\r\n\r\n.ghost-card, .ghost-column {\r\n    opacity: 0.5;\r\n    border: 2px dashed $dark-grey;\r\n\r\n    * {\r\n        background-color: transparent;\r\n        color: transparent;\r\n        fill: transparent;\r\n        opacity: 0;\r\n    }\r\n}\r\n","// CONTENT CARDS\r\n// flexbox source https://developer.mozilla.org/en-US/docs/Web/CSS/CSS_Flexible_Box_Layout/Mastering_Wrapping_of_Flex_Items\r\n#content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    overflow: hidden auto;\r\n\r\n    // SCROLLBAR\r\n    &::-webkit-scrollbar {\r\n        width: 18px; /* width of the entire scrollbar */\r\n    }\r\n\r\n    &::-webkit-scrollbar-track {\r\n        background: $dark-white; /* color of the tracking area */\r\n    }\r\n\r\n    &::-webkit-scrollbar-thumb {\r\n        background-color: $grey; /* color of the scroll thumb */\r\n        border-radius: $big-radius; /* roundness of the scroll thumb */\r\n        border: 4px solid $dark-white; /* creates padding around scroll thumb */\r\n    }\r\n    \r\n    &::-webkit-scrollbar-thumb:hover {\r\n        background-color: $dark-grey;\r\n    }\r\n\r\n    div.group {\r\n        display: flex;\r\n        flex-direction: column;\r\n        margin: 36px;\r\n\r\n        div.group-title {\r\n            display: flex;\r\n            justify-content: flex-start;\r\n\r\n            background-color: $dark-white;\r\n            padding: 12px 0;\r\n            border-radius: $big-radius;\r\n\r\n            h3, input {\r\n                font-weight: 500;\r\n                font-size: 16px;\r\n                color: $dark-grey;\r\n            }\r\n\r\n            input {\r\n                width: auto;\r\n                background-color: transparent;\r\n                border: none;\r\n                padding: 2px;\r\n                text-decoration: underline dotted transparent;\r\n\r\n                &:focus {\r\n                    text-decoration: none;\r\n                }\r\n\r\n                &:hover {\r\n                    cursor: text;\r\n                    text-decoration: underline dotted $dark-grey;\r\n                }\r\n            }\r\n        }\r\n\r\n        div.tiles {\r\n            display: flex;\r\n            flex-wrap: wrap;\r\n            margin: -12px;\r\n\r\n            article.tile {\r\n                min-width: 260px;\r\n                max-width: 280px;\r\n                flex: 1 1 280px;\r\n                margin: 12px;\r\n                padding: 16px;\r\n\r\n                border-radius: $big-radius;\r\n                background-color: $white;\r\n                \r\n                filter: drop-shadow(0 0 2px #30303018);\r\n            }\r\n        }\r\n    }\r\n}\r\n","// BUTTON FORMATTING\r\n.button {\r\n    padding: 12px 18px;\r\n    border-radius: $mini-radius;\r\n    font-weight: 500;\r\n    font-size: 14px;\r\n  \r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n\r\n    // variations\r\n    &.active {\r\n        visibility: visible;\r\n    }\r\n\r\n    &:hover {\r\n        cursor: pointer;\r\n    }\r\n\r\n    // adding a bit of space between the buttons\r\n    &:not(:last-child) {\r\n        margin-right: 16px;\r\n    }\r\n\r\n    // BUTTON TYPES\r\n    &.primary {\r\n        color: $white;\r\n        background-color: $primary;\r\n\r\n        &:hover {\r\n            background-color:$primary-hover\r\n        }\r\n\r\n        // hiding the expanded buttons for mobile\r\n        &.icon {\r\n            display: none;\r\n        }\r\n    }\r\n\r\n    &.secondary {\r\n        background-color: $light-grey;\r\n\r\n        &.hover {\r\n            background-color:#dddddd\r\n        }\r\n    }\r\n\r\n}\r\n","// FORMS\r\n\r\n// modal background\r\n.background {\r\n    display: none;\r\n    background-color: #0e1318af;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    z-index: 5;\r\n}\r\n\r\n.edit {\r\n    padding: 18px;\r\n\r\n    &:hover {\r\n        background-color: greenyellow;\r\n        cursor: pointer;\r\n    }\r\n}\r\n\r\n\r\n// FORM\r\nform {\r\n    z-index: 8;\r\n\r\n    display: none;\r\n    position: absolute;\r\n    width: 80%;\r\n    height: 80%;\r\n    top: calc(10% - 18px);\r\n    left: calc(10% - 18px);\r\n    padding: 36px;\r\n    flex-direction: column;\r\n    background-color: $white;\r\n    border-radius: $big-radius;\r\n\r\n    &.active {\r\n        display: flex;\r\n    }\r\n\r\n    button {\r\n        &#edit-task-delete {\r\n            display: none;\r\n        }\r\n\r\n        &.close-form {\r\n            position: absolute;\r\n            top: 24px;\r\n            right: 24px;\r\n            padding: 12px;\r\n            display: flex;\r\n            justify-content: center;\r\n\r\n            background-color: $white;\r\n            border-radius: $mini-radius;\r\n\r\n            &:hover {\r\n       