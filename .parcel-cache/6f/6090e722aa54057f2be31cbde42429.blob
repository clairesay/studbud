x 0 0;\r\n            box-shadow: 0px 0px 0px 1px #0e131809 inset;\r\n        \r\n            color: $dark-grey;\r\n            background-color: $white;\r\n\r\n            &#tasks-tab {\r\n                left: 135px;\r\n                content: \"Tasks\";\r\n            }\r\n            \r\n            &#content-tab {\r\n                left: 247px;\r\n                content: \"Content\";\r\n            }\r\n\r\n            &:hover {\r\n                cursor: pointer;\r\n            }\r\n\r\n            &.active {\r\n                color: $black;\r\n                background-color: $dark-white;\r\n\r\n                // covering up the drop-shadow with active tabs\r\n                &::before {\r\n                    position: absolute;\r\n                    left: 1.5px;\r\n                    bottom: 0;\r\n                    content: \"\";\r\n                    height: 8px;\r\n                    width: calc(100% - 3px);\r\n                    background-color: $dark-white;\r\n                    z-index: 3;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n","// KANBAN BOARD\r\n#tasks {\r\n    max-width: 100%;\r\n    scroll-snap-type: x mandatory;\r\n    overflow: scroll hidden;\r\n\r\n    // SCROLLBAR\r\n    &::-webkit-scrollbar {\r\n        width: 18px; /* width of the entire scrollbar */\r\n    }\r\n\r\n    &::-webkit-scrollbar-track {\r\n        background: $light-grey; /* color of the tracking area */\r\n    }\r\n\r\n    &::-webkit-scrollbar-thumb {\r\n        background-color: $grey; /* color of the scroll thumb */\r\n        border-radius: $big-radius; /* roundness of the scroll thumb */\r\n        border: 4px solid $light-grey; /* creates padding around scroll thumb */\r\n    }\r\n    \r\n    &::-webkit-scrollbar-thumb:hover {\r\n        background-color: $dark-grey;\r\n    }\r\n\r\n    // COLUMNS\r\n    div.column {\r\n        position: relative;\r\n        min-width: 280px;\r\n        max-width: 332px;\r\n\r\n        margin